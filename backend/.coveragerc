[run]
source = app
branch = True
omit =
    */tests/*
    */__pycache__/*
    */conftest.py
    */fixtures.py
    */__init__.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 80
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
